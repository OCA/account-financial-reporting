<?xml version="1.0" ?>
<openerp>
  <data noupdate="1">

    <record id="mis_report_budget_01" model="mis.report">
      <field name="name">Actuals vs Budget</field>
    </record>

    <record id="mis_report_budget_01_kpi_01" model="mis.report.kpi">
      <field name="name">income</field>
      <field name="description">Income</field>
      <field name="expression">-bal[X20%]</field>
      <field name="sequence" eval="1"/>
      <field name="budgetable" eval="1"/>
      <field name="report_id" ref="mis_report_budget_01"/>
    </record>

    <record id="mis_report_budget_01_kpi_02" model="mis.report.kpi">
      <field name="name">expense</field>
      <field name="description">Expense</field>
      <field name="expression">bal[X21%]</field>
      <field name="sequence" eval="2"/>
      <field name="budgetable" eval="1"/>
      <field name="report_id" ref="mis_report_budget_01"/>
    </record>

  </data>
</openerp>
