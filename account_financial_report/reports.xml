    </record>
    <record
        id="action_print_report_aged_partner_balance_qweb"
        model="ir.actions.report"
    >
        <field name="paperformat_id" ref="report_qweb_paperformat" />
    </record>
    <record id="action_print_report_vat_report_qweb" model="ir.actions.report">
        <field name="paperformat_id" ref="report_qweb_paperformat" />
    </record>
